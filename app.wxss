/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.h1 {
  color: #000000;
  font-weight: 300;
  letter-spacing: 2px;
  text-transform: uppercase;
  text-align: center;
  padding-bottom: 5px;
}
.p {
    color: #696969;
    flex: 1 1 auto;
    line-height: 1.5;
    font-weight: 300;
    letter-spacing: 1px;
    margin-bottom: 1.25rem;
}
.p2{
  color: #000000;
  flex: 1 1 auto;
  line-height: 1.5;
  font-weight: 350;
  letter-spacing: 1px;
  margin-bottom: 1.25rem;
}
.page-wrapper {
  background: #B2C7A8;
  min-height: 100vh;
  position: sticky;
  top: 0px;
}
