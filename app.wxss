/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.h1 {
  color: #000000;
  font-weight: 300;
  letter-spacing: 2px;
  text-transform: uppercase;
  text-align: center;
  padding-bottom: 5px;
}

.h2 {
  color: #000000;
  font-weight: 600;
  font-size: small;
  letter-spacing: 1px;
  text-transform: uppercase;
  text-align: center;
  padding-bottom: 5px;
}

.p {
    color: #696969;
    flex: 1 1 auto;
    line-height: 1.5;
    font-weight: 300;
    letter-spacing: 1px;
    margin-bottom: 1.25rem;
}
.p2{
  color: #000000;
  flex: 1 1 auto;
  line-height: 1.5;
  font-weight: 350;
  letter-spacing: 1px;
  margin-bottom: 1.25rem;
}
.page-wrapper {
  background: #B2C7A8;
  min-height: 100vh;
  position: sticky;
  top: 0px;
  padding-bottom: 20px;
}
.coolButton {
  font-weight: 600;
  letter-spacing: 2px;
  text-transform: uppercase;
  display: inline-block;
  padding: 0.7em 1.4em;
  margin: 0 0.3em 0.3em 0;
  border-radius: 0.15em;
  box-sizing: border-box;
  color:#B2C7A8;
  background-color:#F9EDCC;
  box-shadow: inset 0 -1em 0 -0.5em rgba(0,0,0,0.17);
  position: relative;
  border-radius: 10px;
  text-align: center;
 }

